{
  "database": {
    "type": "mysql",
    "host": "localhost",
    "port": 3306,
    "user": "root",
    "password": "your_password",
    "database": "your_database",
    "options": {}
  },
  "backup": {
    "outputDir": "./backups",
    "retentionDays": 7,
    "schedule": "*/30 * * * *",
    "format": "sql"
  },
  "storage": {
    "local": {
      "enabled": true
    },
    "discord": {
      "enabled": true,
      "webhookUrl": "YOUR_DISCORD_WEBHOOK_URL",
      "maxFileSize": 25000000
    }
  },
  "logging": {
    "enabled": false,
    "level": "info",
    "file": "./logs/backup.log"
  }
}
